function concatSelf(word,n) {
    concat = "";
    for(let i = 0; i <= n-1; i++) {
concat += word;
    }
return concat;
}
console.log(concatSelf("Hello", 3));

function(numArray) {
    let sum = 0;
    for(let i = 0; i < numArray.length; i++) {
        sum += numArray[i];
    } if (sum >100) {
        return true;
    } else{
        return false;
    }
}
