let names = ["Sam", "Tommy", "Tim", "Sally", "Buck", "Bob"]
function findaverage(names) {
    let average = 0;
for(let i = 0; i < names.length; i++){
    let currentNum = names[i]
    average += currentNum;
}
average = average/names.length
return average;
}
    

